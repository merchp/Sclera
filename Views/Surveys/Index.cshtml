@model IEnumerable<Sclera2.Models.Survey>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.survey_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.survey_description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.updated)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.survey_name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.survey_description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.updated)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.survey_id }) |
            @Html.ActionLink("Details", "Details", new { id=item.survey_id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.survey_id })
        </td>
    </tr>
}

</table>
